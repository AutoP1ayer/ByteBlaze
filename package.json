{
  "name": "byteblaze",
  "version": "1.0.0",
  "description": "A versatile and powerful music bot for Discord that brings rhythm and melody to your server. This is a big upgrade of Cylane!",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --build --verbose",
    "build:full": "npm-run-all build:prettier build build:languages",
    "build:languages": "tsx ./src/helper/copyFiles.ts",
    "start": "npm-run-all build:full start:bot",
    "start:bot": "node ./dist/index.js",
    "start:docker": "tsx ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "install:musicard": "npm i musicard",
    "build:prettier": "prettier -w ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RainyXeon/ByteBlaze.git"
  },
  "keywords": [
    "discordbot"
  ],
  "author": "RainyXeon",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/RainyXeon/ByteBlaze/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "nodemonConfig": {
    "ignore": [
      "*.database.json"
    ],
    "execMap": {
      "ts": "tsx"
    }
  },
  "homepage": "https://github.com/RainyXeon/ByteBlaze#readme",
  "dependencies": {
    "@hammerhq/localization": "^0.1.5",
    "axios": "^1.4.0",
    "better-kazagumo": "^0.0.4",
    "chalk": "^4.1.2",
    "chillout": "^5.0.0",
    "common-tags": "^1.8.2",
    "delay": "^6.0.0",
    "discord-api-types": "^0.37.61",
    "discord.js": "^14.13.0",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.1.1",
    "humanize-duration": "^3.29.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "moment": "^2.29.4",
    "mongoose": "^6.9.1",
    "musicard": "github:RainyXeon/musicard",
    "mysql2": "^3.6.0",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "plsargs": "^0.1.6",
    "pretty-ms": "^8.0.0",
    "quick.db": "^9.1.7",
    "recursive-readdir": "^2.2.3",
    "shoukaku": "^3.4.0",
    "stuffs": "^0.1.27",
    "voucher-code-generator": "^1.3.0",
    "winston": "^3.9.0",
    "write-file-atomic": "^5.0.1"
  },
  "devDependencies": {
    "@digitak/tsc-esm": "^3.1.4",
    "@types/chillout": "^5.0.1",
    "@types/common-tags": "^1.8.1",
    "@types/express": "^4.17.17",
    "@types/express-cluster": "^0.0.1",
    "@types/express-status-monitor": "^1.3.0",
    "@types/express-ws": "^3.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.195",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^20.4.1",
    "@types/recursive-readdir": "^2.2.1",
    "@types/voucher-code-generator": "^1.1.1",
    "@types/yt-search": "^2.10.0",
    "concurrently": "^8.2.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "node": "^20.7.0",
    "npm": "^8.19.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
